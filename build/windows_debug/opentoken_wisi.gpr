--  For using Wisi OpenToken in other projects
with "../common";
project OpenToken_Wisi is

   for Source_Dirs use
     ("../..",
      "../../wisi");

   for Object_Dir use "obj";

   package Compiler is
      for Default_Switches ("Ada") use
        Common.Compiler.Debug_Switches &
        Common.Compiler.Style_Checks &
        Common.Compiler'Default_Switches ("Ada");
   end Compiler;

end OpenToken_Wisi;
