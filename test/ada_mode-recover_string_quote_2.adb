-- Real error of missing string quote. Used to violate a constraint, fails recovery gracefully.
--
-- FIXME: check for lexer error in language_fixes, insert the quote where it belongs.

--EMACS_SKIP_UNLESS:(eq ada-parser 'process)
--EMACSCMD:(setq skip-recase-test t)
package body Ada_Mode.Recover_String_Quote_2 is
   procedure Handle_Search
   is

   begin
      if URI_Param.Is_Empty then
         return "text/html";

      else

            Response := Response & "</table>"</body></html>";
      end if;
   end Handle_Search;
end Ada_Mode.Recover_String_Quote_2;
