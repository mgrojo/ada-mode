;; test number-literal lexer

%{
%}
%keyword NUMBER "number"

%token <number> NUMERIC_LITERAL wisi-number-p

%start compilation_unit
%parser_algorithm LALR
%output_language Elisp

%%

compilation_unit
  : number_list
    %((setq wisi-test-success t))%
  ;

number_list
  : NUMBER NUMERIC_LITERAL
  | number_list NUMBER NUMERIC_LITERAL
  ;

%%
;;; end of file
