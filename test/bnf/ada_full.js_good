         $.END,
         field('endname',optional($.identifier)),
      ),
      overriding_indicator: $ => seq(
         optional(reserved('not')),
         reserved('overriding'),
      ),
      _parameter_and_result_profile: $ => seq(
         optional($.formal_part),
         $.result_profile,
