         reserved('out'),
      ),
      null_procedure_declaration: $ => seq(
         optional($.overriding_indicator),
         $.procedure_specification,
         $.IS,
         $.NULL,
         optional($.aspect_specification),
         ';',
      ),
      single_task_declaration: $ => seq(
         reserved('task'),
         $.identifier,
         optional($.aspect_specification),
         optional(seq(
            $.IS,
            optional(seq(
               $.NEW,
               $.interface_list,
               $.WITH,
            )),
            $.task_definition,
