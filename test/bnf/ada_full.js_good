         ';',
      ),
      task_type_declaration: $ => seq(     // RM 9.1
         reserved('task'),
         reserved('type'),
         $.identifier,
         optional($.known_discriminant_part),
         optional($.aspect_specification),
         optional(seq(
            $.IS,
            optional(seq(
               $.NEW,
               $.interface_list,
               $.WITH,
            )),
            $.task_definition,
