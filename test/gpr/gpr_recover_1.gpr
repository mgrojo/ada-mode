-- From a real editing session. Recover does a reasonable job.
--EMACS_SKIP_UNLESS:(eq gpr-parser 'process)
project gpr_recover_1 is
   package Linker is
      case Profile is
         when "On" =>
            case Mem_Check is
               when
                  for Linker_Options use ("-pg", "-lgmem");

               when "Off" =>
                  null;
            end case;
      end Linker;

   end gpr_recover_1;
