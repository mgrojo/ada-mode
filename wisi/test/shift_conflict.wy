%{
(require 'wisi)
%}

%keyword BEGIN "begin"
%keyword IS "is"

%token <punctuation> SEMICOLON ";"
%token <line_comment> COMMENT "--"

%start declarations

%%

declarations
  : ;; empty declaration list
  | declarations entry_body
  ;

entry_body
  : IS declarations BEGIN SEMICOLON
  ;

